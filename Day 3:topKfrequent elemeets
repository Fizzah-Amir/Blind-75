vector<int> topKFrequent(vector<int>& nums, int k) {
    unordered_map<int, int> a;
    vector<int>ans;
    for (int i = 0; i < nums.size(); i++) {
        a[nums[i]]++;
    }
    priority_queue<pair<int, int>>m;
    for (auto& p : a) {
        m.push({ p.second,p.first });
    }
    while (k > 0) {
        auto p = m.top();
        int b = p.first;
        int c = p.second;
        ans.push_back(c);
        m.pop();
        k--;
    }
    return ans;
}
