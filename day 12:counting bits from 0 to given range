  vector<int> countBits(int n) {
    int count = 0;
    vector<int>ans;
    for (int i = 0; i <= n; i++) {
        if (i== 0) {
            ans.push_back(0);
            continue;
        }
        int d = i;
        while (d) {
            count++;
            d = d & (d- 1);
        }
        ans.push_back(count);
        count = 0;
    }
    return ans;
